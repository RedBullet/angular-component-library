{
  "extends": "angular",
  "plugins": [
    "angular"
  ],
  "env": {
    "es6": true
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  },
  "rules": {
    "arrow-body-style": [2, "always"],
    "import/no-unresolved": 0
  }
}
